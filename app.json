{
  "expo": {
    "name": "AiPair",
    "slug": "expoAiPair",
    "version": "1.4.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.kapernikrs.aipair",
      "buildNumber": "36",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library so you can upload images to your profile and events.",
        "NSCameraUsageDescription": "We need access to your camera so you can take photos directly in the app.",
        "NSLocationWhenInUseUsageDescription": "We need access to your location to show events near you.",
        "NSUserTrackingUsageDescription": "We use your device identifier to deliver more relevant ads.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./android/app/google-services.json",
      "package": "com.kapernikrs.aipair",
      "versionCode": 36
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#FFFFFF"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.728122372039-a6jj9godm62p3o7vov8f7q5psjfppiu7"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8636022279548301~3339084593",
          "iosAppId": "ca-app-pub-8636022279548301~4752416229"
        }
      ],
      "expo-apple-authentication",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "08a4b737-a378-4629-8a11-a5c2bcd62c3f"
      }
    }
  }
}